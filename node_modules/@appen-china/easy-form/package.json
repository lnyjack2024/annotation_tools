{
  "name": "@appen-china/easy-form",
  "version": "1.10.6",
  "files": [
    "dist",
    "es"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@ant-design/icons": "4.8.0",
    "antd": "4.24.5",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "pinyin-match": "^1.1.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@types/jest": "^29.5.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@vitejs/plugin-react": "^4.1.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "less": "^3.12.2",
    "postcss": "^8.4.31",
    "rollup": "^2.30.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "typescript": "4.9.4",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "dev": "vite serve dev --port 7001",
    "build": "npm run build:rollup && npm run build:es",
    "build:rollup": "npx rollup -c && npm run build:theme",
    "build:theme": "lessc src/styles/light.less dist/styles/light.css --js && lessc src/styles/dark.less dist/styles/dark.css --js",
    "build:es": "npm run build:types && npm run build:js && npm run build:styles",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir es --extensions \".ts,.tsx\" --source-maps",
    "build:styles": "cp -r src/styles es && lessc src/styles/light.less es/styles/light.css --js && lessc src/styles/dark.less es/styles/dark.css --js",
    "prepublishOnly": "rm -rf dist && rm -rf es && npm run build",
    "publish:local": "npm run prepublishOnly"
  },
  "gitHead": "2605d53b6f6dbacddc1c8bfc800b99bd2a23cb51"
}
